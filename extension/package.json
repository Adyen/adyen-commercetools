{
  "name": "commercetools-adyen-integration-extension",
  "version": "6.0.1",
  "description": "Integration between Commercetools and Adyen payment service provider",
  "license": "MIT",
  "scripts": {
    "check-coverage": "nyc check-coverage --statements 97",
    "test": "npm run lint && npm run cover:unit && npm run cover:integration && npm run cover:e2e && npm run check-coverage",
    "cover:unit": "nyc --silent npm run unit",
    "cover:integration": "nyc --silent --no-clean npm run integration",
    "cover:e2e": "nyc --silent --no-clean npm run e2e",
    "unit": "nyc mocha test/unit/**/*.spec.js",
    "integration": "nyc mocha test/integration/**/*.spec.js",
    "e2e": "nyc mocha test/e2e/**/*.spec.js",
    "start": "node ./src/init.js",
    "lint": "eslint ./src ./test",
    "create-custom-types": "node -e 'require(\"./src/config/init/ensure-resources.js\").ensureCustomTypes()'",
    "doctoc": "doctoc . --github",
    "format": "prettier --write ."
  },
  "keywords": [
    "Adyen",
    "commercetools",
    "payment",
    "payment integration",
    "payment provider"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercetools/commercetools-adyen-integration.git"
  },
  "author": {
    "name": "commercetools Professional Services",
    "email": "ps-dev@commercetools.com"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/commercetools/commercetools-adyen-integration/issues"
  },
  "homepage": "https://github.com/commercetools/commercetools-adyen-integration",
  "devDependencies": {
    "doctoc": "^2.0.0",
    "eslint": "^5.16.0",
    "mocha": "^8.2.1",
    "ngrok": "^3.4.0",
    "nyc": "^15.1.0",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "commercetools-adyen-integration-commons": "1.0.0"
  },
  "mocha": {
    "timeout": 40000,
    "file": "./test/cleanup-ctp-project.js",
    "exit": true,
    "full-trace": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
