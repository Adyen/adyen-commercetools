{
  "key": "ctp-adyen-integration-payment-extension",
  "destination": {
    "type": "HTTP",
    "url": "${ctpAdyenIntegrationBaseUrl}"
  },
  "triggers": [
    {
      "resourceTypeId": "payment",
      "actions": ["Create", "Update"],
      "condition": "paymentMethodInfo(paymentInterface=\"ctp-adyen-integration\")"
    }
  ],
  "timeoutInMs": 10000
}
